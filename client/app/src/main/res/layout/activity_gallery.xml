<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:id="@+id/main">

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/photoList"
        android:layout_width="match_parent"
        android:layout_height="500dp"
        android:layout_marginTop="138dp"
        android:gravity="center"
        android:padding="5dp"
        app:layoutManager="androidx.recyclerview.widget.GridLayoutManager"
        app:spanCount="1" />

</LinearLayout>
    <View
        android:layout_width="417dp"
        android:layout_height="2dp"
        android:background="#D3D3D3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.18" />

    <TextView
        android:id="@+id/date"
        android:layout_width="202dp"
        android:layout_height="50dp"
        android:layout_marginTop="28dp"
        android:fontFamily="@font/notosans_medium"
        android:gravity="center"
        android:text="date"
        android:textColor="@color/black"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/empty"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginTop="320dp"
        android:gravity="center"
        android:text="비어있음"
        android:textSize="18dp"
        android:fontFamily="@font/notosans_medium"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <!--<FrameLayout
        android:id="@+id/main_container"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/bottom_navigation_view"
        app:layout_constraintEnd_toEndOf="@+id/bottom_navigation_view"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />
-->

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/move"
        android:layout_width="90dp"
        android:layout_height="32dp"
        android:layout_marginTop="90dp"
        android:background="@drawable/edittext"
        android:drawableLeft="@drawable/sizetrash"
        android:scaleType="fitCenter"
        android:padding="2dp"
        android:text="사진 이동"
        android:elevation="5dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.99"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.widget.Button
        android:id="@+id/moveOk"
        android:layout_width="380dp"
        android:layout_height="45dp"
        android:layout_marginTop="556dp"
        android:background="@drawable/select"
        android:elevation="10dp"
        android:fontFamily="@font/notosans_bold"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:stateListAnimator="@null"
        android:text="앨범 선택하기"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/photoPlus"
        android:layout_width="100dp"
        android:layout_height="32dp"
        android:layout_marginTop="90dp"
        android:background="@drawable/edittext"
        android:drawableLeft="@drawable/sizephotoplus"
        android:scaleType="fitCenter"
        android:text="사진 추가"
        android:padding="2dp"
        android:elevation="5dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.69"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/photoTrash"
        android:layout_width="105dp"
        android:layout_height="32dp"
        android:layout_marginTop="90dp"
        android:background="@drawable/edittext"
        android:drawableLeft="@drawable/sizetrash"
        android:scaleType="fitCenter"
        android:text="사진 휴지통"
        android:elevation="5dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.35"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/chat"
        android:layout_width="100dp"
        android:layout_height="32dp"
        android:layout_marginTop="90dp"
        android:background="@drawable/edittext"
        android:drawableLeft="@drawable/sizechat"
        android:scaleType="fitCenter"
        android:text="그룹 게시판"
        android:elevation="5dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.01"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:layout_width="417dp"
        android:layout_height="2dp"
        android:background="#D3D3D3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.111" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottom_navigation_view"
        android:layout_width="410dp"
        android:layout_height="85dp"
        app:itemIconTint="#2196F3"
        app:itemTextColor="@color/black"
        app:labelVisibilityMode="labeled"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/menu_bottom_nav" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/albumList"
        android:layout_width="350dp"
        android:layout_height="410dp"
        android:layout_marginTop="180dp"
        android:layout_marginStart="28dp"
        android:background="@drawable/text"
        android:gravity="center"
        android:padding="0dp"
        android:elevation="30dp"
        android:visibility="invisible"
        app:layoutManager="androidx.recyclerview.widget.GridLayoutManager"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.408"
        app:spanCount="2" />

    <android.widget.Button
        android:id="@+id/save"
        android:layout_width="80dp"
        android:layout_height="35dp"
        android:layout_marginTop="550dp"
        android:layout_marginStart="-80dp"
        android:elevation="30dp"
        android:padding="0dp"
        android:text="이동하기"
        android:background="#00000000"
        android:fontFamily="@font/notosans_bold"
        android:scaleType="fitCenter"
        android:stateListAnimator="@null"
        android:visibility="invisible" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>