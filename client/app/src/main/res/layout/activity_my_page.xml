<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:background="#e6f4f1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.AlbumMain">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="MissingConstraints,Orientation">

    <View
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:layout_marginTop="250dp"
        android:background="#F0F8FF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:background="#00000000"
        android:layout_marginTop="260dp"
        android:scaleType="fitCenter"
        android:src="@drawable/profile"
        android:text="이름"
        android:layout_marginStart="-360dp"
        android:fontFamily="@font/ibm_medium"
        app:tint="#234567" />

    <TextView
        android:id="@+id/name"
        android:layout_width="329dp"
        android:layout_height="45dp"
        android:layout_marginTop="298dp"
        android:layout_marginStart="-100dp"
        android:textSize="15sp"
        android:padding="5sp"
        android:background="@drawable/text"
        android:fontFamily="@font/ibm_regular"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.451"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:background="#00000000"
        android:layout_marginTop="360dp"
        android:layout_marginStart="-330dp"
        android:scaleType="fitCenter"
        android:src="@drawable/profile"
        android:text="이메일"
        android:fontFamily="@font/ibm_medium"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.118"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.499"
        app:tint="#234567" />

    <TextView
        android:id="@+id/email"
        android:layout_width="329dp"
        android:layout_height="45dp"
        android:layout_marginTop="400dp"
        android:layout_marginStart="-100dp"
        android:background="@drawable/text"
        android:padding="5dp"
        android:textSize="15sp"
        android:fontFamily="@font/ibm_regular"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.451"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/profile"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="50dp"
        android:src="@drawable/profile"
        android:layout_marginStart="-240dp"
        android:scaleType="centerCrop"
        android:adjustViewBounds="true" />

    <ImageButton
        android:id="@+id/profileadd"
        android:layout_width="31dp"
        android:layout_height="36dp"
        android:layout_marginTop="170dp"
        android:layout_marginStart="-30dp"
        android:background="#00000000"
        android:scaleType="fitCenter"
        android:src="@drawable/profileadd"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.673"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.237"/>

    <android.widget.ImageButton
        android:id="@+id/retouch"
        android:layout_width="40dp"
        android:layout_height="35dp"
        android:background="@drawable/edit"
        android:layout_marginTop="452dp"
        android:layout_marginStart="75dp"
        android:scaleType="fitCenter"
        android:elevation="20dp"/>

    <Switch
        android:id="@+id/switch1"
        android:layout_width="wrap_content"
        android:layout_height="31dp"
        android:track="@drawable/selector"
        android:thumb="@drawable/thumb"
        android:layout_marginTop="200dp"
        android:layout_marginStart="-250dp"
        android:text="검색 허용"
        android:fontFamily="@font/ibm_medium"
        app:track="@drawable/track"
        app:thumbTextPadding="24dp"
        app:switchMinWidth="0dp" />

    <android.widget.Button
        android:id="@+id/logout"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:layout_marginTop="535dp"
        android:layout_marginStart="-260dp"
        android:background="@drawable/text2"
        android:fontFamily="@font/ibm_medium"
        android:textColor="@color/black"
        android:scaleType="fitCenter"
        android:text="로그아웃"/>

    <android.widget.Button
        android:id="@+id/secession"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:layout_marginStart="-400dp"
        android:layout_marginTop="570dp"
        android:background="@drawable/text2"
        android:fontFamily="@font/ibm_medium"
        android:padding="0dp"
        android:backgroundTint="#f05659"
        android:scaleType="fitCenter"
        android:text="탈퇴하기"
        android:textColor="@color/black" />
</LinearLayout>

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottom_navigation_view"
        android:layout_width="match_parent"
        android:layout_height="65dp"
        app:itemTextColor="@color/black"
        app:labelVisibilityMode="unlabeled"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/menu_bottom_nav" />

</androidx.constraintlayout.widget.ConstraintLayout>